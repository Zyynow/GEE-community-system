<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jxufe.dao.FeedbackMapper">

    <select id="pageFeedbacks" resultType="com.jxufe.entity.Feedback">
        select *
        from t_feedback
    </select>

    <delete id="deleteFeedback">
        delete
        from t_feedback
        where id = #{id}
    </delete>

</mapper>